<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sumauto.cube">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="Manifest.permission.PACKAGE_USAGE_STATS" />
    <permission android:name="talk.to.sumauto" />
    <uses-permission android:name="talk.to.sumauto" />

    <application
        android:name="com.sumauto.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">

        <activity android:name=".AndroidSDKActivity" />

        <activity android:name=".rx.RxActivity" />

        <activity android:name=".flow.WorkFlowTest" />

        <service
            android:name=".acessbility.CubeAccessibilityService"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>

            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/accessibilityservice" />
        </service>

        <activity android:name=".shell.ShellActivity" />
        <activity android:name=".DexActivity" />
        <activity android:name=".BatteryActivity" />
        <activity android:name=".coroutines.CoroutinesActivity" />
        <activity android:name="com.sumauto.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:mimeType="*/*"
                    android:scheme="content" />
            </intent-filter>
        </activity>
    </application>

</manifest>